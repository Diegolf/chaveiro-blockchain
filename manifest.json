{
    "name": "Chaveiro Blockchain",
    "version": "1.0",
    "description": "Extens√£o utilizada para manipular o chaveiro no Blockchain da ethereum.",
    "manifest_version": 2,
    "permissions":[
        "notifications",
        "contextMenus",
        "tabs",
        "activeTab"
    ],
    "browser_action":{
        "default_popup": "view/popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "background":{
        "scripts":[
           "js/jquery-3.3.1.min.js",
           "js/web3.min.js",
           "js/aes.js",
           "js/script.js"
        ],
        "persistent": false
    }
}